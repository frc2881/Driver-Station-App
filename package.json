{
  "name": "frc-driver-station-app",
  "description": "FRC driver station app for HUD, dashboard, and NT data inspection",
  "version": "2025.0.1",
  "author": "frc2881",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "app/main.js",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "npm-run-all build:*",
    "build:init": "node scripts/build-init.mjs",
    "build:main": "tsc -p tsconfig-main.json",
    "build:ui": "rollup -c",
    "dist": "electron-builder --win",
    "package": "npm-run-all build dist",
    "start": "electron .",
    "start:local": "electron . --serverAddress=127.0.0.1",
    "watch": "rollup -cw",
    "dev": "npm-run-all build --parallel start watch",
    "dev:local": "npm-run-all build --parallel start:local watch"
  },
  "build": {
    "appId": "FRC.DriverStationApp.2025.0.1",
    "productName": "FRCDriverStationApp",
    "copyright": "Copyright (c) FRC Team 2881",
    "win": {
      "target": "portable",
      "artifactName": "${productName}-${version}.${ext}",
      "compression": "store",
      "icon": "resources/app-icon.png"
    }
  },
  "dependencies": {
    "ws": "8.18.0",
    "@msgpack/msgpack": "2.8.0",
    "minimist": "1.2.8",
    "date-fns": "4.1.0"
  },
  "devDependencies": {
    "@types/ws": "8.5.12",
    "@types/minimist": "1.2.5",
    "npm-run-all": "4.1.5",
    "electron": "33.0.2",
    "electron-builder": "25.1.8",
    "@electron/packager": "18.3.5",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "rollup": "4.24.0",
    "rollup-plugin-css-only": "4.5.2",
    "rollup-plugin-svelte": "7.2.2",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.1",
    "@rollup/plugin-replace": "6.0.1",
    "svelte": "5.1.0",
    "svelte-preprocess": "6.0.3",
    "carbon-preprocess-svelte": "0.11.7",
    "carbon-components-svelte": "0.85.2",
    "carbon-icons-svelte": "12.12.0",
    "carbon-pictograms-svelte": "12.11.0",
    "@carbon/charts-svelte": "1.22.0",
    "d3": "7.9.0",
    "d3-cloud": "1.2.7",
    "d3-sankey": "0.12.3"
  },
  "engines": {
    "node": "20.18.0"
  },
  "volta": {
    "node": "20.18.0"
  }
}
