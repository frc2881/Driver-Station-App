{
  "name": "frc-driver-station-app",
  "description": "FRC driver station app for HUD, dashboard, and NT data inspection",
  "version": "2024.1.0",
  "author": "frc2881",
  "license": "MIT",
  "private": true,
  "main": "app/main.js",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "npm-run-all build:*",
    "build:init": "node scripts/build-init.mjs",
    "build:main": "tsc -p tsconfig-main.json",
    "build:ui": "rollup -c --bundleConfigAsCjs",
    "dist": "electron-builder --win",
    "package": "npm-run-all build dist",
    "start": "electron .",
    "watch": "rollup -cw --bundleConfigAsCjs",
    "dev": "npm-run-all build --parallel start watch"
  },
  "build": {
    "appId": "FRC.DriverStationApp.2024.1.0",
    "productName": "FRCDriverStationApp",
    "copyright": "Copyright (c) FRC Team 2881",
    "win": {
      "target": "portable",
      "artifactName": "${productName}-${version}.${ext}",
      "compression": "store",
      "icon": "resources/app-icon.png"
    }
  },
  "dependencies": {
    "ws": "^8.17.0",
    "@msgpack/msgpack": "^2.8.0",
    "minimist": "^1.2.8",
    "dayjs": "^1.11.11",
    "plotly.js-basic-dist-min": "^2.32.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "@types/minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "fs-extra": "^11.2.0",
    "electron": "^30.0.8",
    "electron-packager": "^17.1.2",
    "electron-builder": "^24.13.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "@tsconfig/svelte": "^5.0.4",
    "rollup": "^4.18.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-svelte": "^7.2.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-replace": "^5.0.5",
    "svelte": "^4.2.17",
    "svelte-preprocess": "^5.1.4",
    "carbon-preprocess-svelte": "^0.11.3",
    "carbon-components-svelte": "^0.85.0",
    "carbon-icons-svelte": "^12.6.0",
    "carbon-pictograms-svelte": "^12.11.0",
    "@carbon/charts-svelte": "1.16.1",
    "d3": "7.9.0",
    "d3-cloud": "1.2.7",
    "d3-sankey": "0.12.3",
    "svelte-plotly.js": "^0.3.2",
    "@types/plotly.js": "^2.29.4"
  },
  "volta": {
    "node": "20.13.1",
    "pnpm": "9.1.2"
  }
}
