{
  "name": "frc-driver-station-app",
  "description": "FRC driver station app for HUD, dashboard, and NT data inspection",
  "version": "2025.7.1",
  "author": "frc2881",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "app/main.js",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "npm-run-all build:*",
    "build:init": "node scripts/build-init.mjs",
    "build:main": "tsc -p tsconfig-main.json",
    "build:ui": "rollup -c",
    "dist": "electron-builder --win",
    "package": "npm-run-all build dist",
    "start": "electron .",
    "dev": "npm-run-all build --parallel watch \"start -- {1}\" --",
    "watch": "rollup -cw"
  },
  "build": {
    "appId": "FRC.DriverStationApp.2025.7.1",
    "productName": "FRCDriverStationApp",
    "copyright": "Copyright (c) FRC Team 2881",
    "win": {
      "target": "portable",
      "artifactName": "${productName}-${version}.${ext}",
      "compression": "store",
      "icon": "resources/app-icon.png"
    }
  },
  "dependencies": {
    "ws": "8.18.3",
    "@msgpack/msgpack": "3.1.2",
    "date-fns": "4.1.0"
  },
  "devDependencies": {
    "@types/ws": "8.18.1",
    "npm-run-all": "4.1.5",
    "electron": "37.2.6",
    "electron-builder": "26.0.12",
    "@electron/packager": "18.4.0",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "rollup": "4.46.2",
    "rollup-plugin-css-only": "4.5.2",
    "rollup-plugin-svelte": "7.2.2",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@rollup/plugin-replace": "6.0.2",
    "svelte": "5.38.0",
    "svelte-preprocess": "6.0.3",
    "carbon-preprocess-svelte": "0.11.11",
    "carbon-components-svelte": "0.89.5",
    "carbon-icons-svelte": "13.5.0",
    "carbon-pictograms-svelte": "13.8.0",
    "@carbon/charts-svelte": "1.22.18",
    "d3": "7.9.0",
    "d3-cloud": "1.2.7",
    "d3-sankey": "0.12.3"
  },
  "engines": {
    "node": "22.16.0"
  },
  "volta": {
    "node": "22.16.0"
  }
}
